"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\v_sapper/v_sapper_glass"
	"$bumpmap" "models/weapons\v_sapper\v_sapper_glass_normal"

	"$translucent" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.799821211 .78568867 .761431507]"
	"$normalmapalphaenvmapmask" 1

	"$phong" "1"
	"$phongexponent" "125"
	"$phongboost" "10"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges" "[.25 .5 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}		
	}
}