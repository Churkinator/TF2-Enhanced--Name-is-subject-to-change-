"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_flameball/c_flameball_blue"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "3"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"		
	"$phongfresnelranges"	"[.5 1 2]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "14"
	"$rimlightboost" ".35"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"WeaponSkin"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}