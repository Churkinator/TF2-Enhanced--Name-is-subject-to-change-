"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_smg/c_smg"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongboost" "2.5"
	"$phongexponenttexture" "models/weapons/c_models/c_smg/c_smg_exp"	
	"$phongexponentfactor"  "120"
	"$phongalbedotint" "1"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges" "[.3 .5 3]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "13"
	"$rimlightboost" "2.25"
	"$rimmask" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"WeaponSkin"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}