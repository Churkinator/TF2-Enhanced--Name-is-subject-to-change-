"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_ambassador/c_ambassador_blue_gold"
	"$bumpmap" "models/weapons/c_models/c_ambassador/c_ambassador_normal"

	"$envmap" "cubemaps/cubemap_gold001"
	"$envmaptint" "[2.647471805 2.541 0]"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongexponent" "125"
	"$phongboost" "10"	
	"$lightwarptexture" "models/weapons/c_models/c_ambassador/c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$phongwarptexture"  "models/weapons/c_models/c_ambassador/c_ambassador_phongwarp"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "2"	
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}