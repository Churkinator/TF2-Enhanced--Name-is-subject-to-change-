"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_models/c_shotgun/c_shotgun"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongboost" "2.5"
	"$phongexponenttexture" "models/weapons/c_models/c_shotgun/c_shotgun_exp"
	"$phongexponentfactor"  "150"
	"$phongalbedotint" "1"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"	
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" "2.25"
	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"
	"$glowcolor" "1"
	
	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}