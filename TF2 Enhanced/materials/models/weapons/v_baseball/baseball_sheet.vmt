"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\v_baseball\baseball_sheet"
	"$bumpmap" "models\weapons\v_baseball\baseball_normal"

	"$phong" "1"
	"$phongexponent" "3"
	"$phongboost" "1.3"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.05 .15 .25]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"
	"$glowcolor" "1"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}