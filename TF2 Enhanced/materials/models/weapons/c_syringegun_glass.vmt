"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_syringegun_glass"
//Решил не использовать никакие маски для Фонга, потому что так будет лучше для зеркальных отражений, наверное
//	"$bumpmap" "models/player/shared/shared_normal"

	"$translucent" "1"
	"$additive" "1"

	"$envmap" "env_cubemap"
//	"$envmaptint" [.5 .5 .5]
	"$envmapsaturation" "1"
	"$basealphaenvmapmask" "1"
	"$invertphongmask" "1" //Actually inverts envmask, instead of phongmask

	"$phong" "1"
	"$phongexponent" "125"
	"$phongboost" ".7"
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$phongfresnelranges"	"[1.5 3 6]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "20"
	"$rimlightboost" ".25"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}