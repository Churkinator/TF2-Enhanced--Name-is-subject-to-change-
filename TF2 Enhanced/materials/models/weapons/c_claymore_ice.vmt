"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_claymore_ice"
	"$bumpmap" "models/player/shared/shared_normal"

	"$translucent" "1"

	"$envmap" "env_cubemap"
//	"$envmaptint" "[.3 .3 .2]"
	"$envmaptint" "[.679549431 .788065231 .907561214]"
	"$normalmapalphaenvmapmask" "1"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongexponent" "150"
	"$phongboost" "10"
	"$lightwarptexture" "models\props_forest/icicle_lightwarp"	
	"$phongfresnelranges" "[.3 .5 3]"
	"$halflambert" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "7.5"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$glowcolor" "1"	
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}