"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_urinejar_urine"
	"$bumpmap" "models/player/shared/shared_normal"

//	"$additive" "1"
	"$nocull" "1"
	"$translucent" "1"

	"$envmap" "env_cubemap"
//	"$envmaptint" "[.75 .75 0]"
	"$envmaptint" "[1 .881 .46]"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongboost" "1.5"
	"$phongtint" "{255 196 43}"
	"$phongexponent" "85"
	"$phongfresnelranges" "[1 2 3]"
//	"$lightwarptexture" "models\lightwarps\jarate_lightwarp"
	"$lightwarptexture" "models\lightwarps\softened_weapon_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "20"
	"$rimlightboost" ".25"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
