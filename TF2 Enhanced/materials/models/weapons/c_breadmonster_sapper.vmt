"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items\c_breadmonster_sapper"
	"$bumpmap" "models\weapons\c_items\c_breadmonster_sapper_normal"

	"$envmap" "env_cubemap"
//	"$envmaptint" "[.5 .4 .1]"
	"$envmaptint" "[.90756 .73621 .5061]"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "5"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}