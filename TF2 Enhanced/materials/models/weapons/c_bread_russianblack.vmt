"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_bread_russianblack"
	"$bumpmap" "models/weapons/c_items/c_bread_russianblack_normal"

	"$phong" "1"
	"$phongexponent" "8"
	"$phongboost" "2"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.15 .25 .5]"


	"$rimlight" "1"
	"$rimlightexponent" "15"	
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"
	"$glowcolor" "1"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}