"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_proto_medigun_glass"
	"$bumpmap" "models/weapons/c_items/c_proto_medigun_glass_normal"

	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.58 0.58 0.58]"
	"$envmapfresnel" "1"
	"$envmapsaturation" "1"

	"$translucent" "1"

	//phong properties
	"$phong"	"1"
	"$phongboost"	"2"
	"$phongexponent"	"30"
	"$phongfresnelranges"	"[.5 1 2]"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"		
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "20"
	"$rimlightboost" "2"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}