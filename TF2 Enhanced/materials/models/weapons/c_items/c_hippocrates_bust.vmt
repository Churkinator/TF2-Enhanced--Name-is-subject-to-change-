"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_hippocrates_bust"
	"$bumpmap" "models/weapons/c_items/c_hippocrates_bust_normal"

	"$envmap"	"env_cubemap"
	"$envmaptint" "[.792792731 .712588358 .394776879]"
//	"$normalmapalphaenvmapmask" "1"
	"$envmapsaturation" "1"
	
	"$lightwarptexture" "models\weapons/w_syringe/w_syringe_lightwarp"

	//phong properties
	"$phong"	"1"
	"$phongboost"	"10"
	"$phongexponent"	"120"
	"$phongfresnelranges"	"[.5 1 2]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}