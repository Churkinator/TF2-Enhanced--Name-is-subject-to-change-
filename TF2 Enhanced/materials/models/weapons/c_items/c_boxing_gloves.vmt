"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_boxing_gloves"
	"$bumpmap" "models\weapons\c_items/c_boxing_gloves_normal"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "4"	
	"$lightwarptexture" "models/player/engineer/engineer_hands_lightwarp"
	"$phongfresnelranges"	"[.25 .55 3]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "3"
	"$rimlightboost" ".2"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}