"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_urinejar_glass"
	"$bumpmap" "models/weapons/c_items/c_urinejar_glass_phongmask"

	"$additive"	"1"

	"$envmap" "env_cubemap"
//	"$envmaptint" "[.3 .3 .2]"
	"$envmaptint" "[.9312 .907 .858]"
	"$envmapsaturation" "1"
	"$normalmapalphaenvmapmask" "1"

	"$phong" "1"
//	"$phongboost" "6"
//	"$phongexponent" "65"
	"$phongboost" "2.5"
	"$phongexponent" "100"
	"$phongfresnelranges" "[1 2 4]"
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "2"

	"$glowcolor" "1"
	"$yellow" "0"
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
