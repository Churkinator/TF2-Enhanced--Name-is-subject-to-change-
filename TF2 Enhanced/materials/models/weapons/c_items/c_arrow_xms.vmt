"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_arrow_xmas"
	"$bumpmap" "models/player/shared/shared_normal"

	"$selfillum" "1"

	"$nocull" "1"

	"$phong" "1"
	"$phongexponent" "150"
	"$phongboost" "5"	
	"$lightwarptexture" "models/lightwarps/xms_lightwarp"
	"$phongfresnelranges"	"[1 1 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" "7.5"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$yellow" "0"
	"$glowcolor" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"Sine"
		{
			"resultVar"	"$selfillumtint" 
		    "sineperiod" "5"
			"sinemin" ".3"
			"sinemax" "1"
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 1
		}	
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}	
}
