"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_sandwich_xmas_blue"
	"$bumpmap" "models\weapons\c_items/c_sandwich_xmas_normal"

	//phong properties
	"$phong"	"1"
	"$phongboost"	"1"
	"$phongexponent"	"10"
	"$phongfresnelranges"	"[1 2 2.5]"
	"$halflambert" "1"
	"$lightwarptexture" "models/lightwarps/xms_lightwarp"

	"$rimlight"	"1"
	"$rimlightexponent"	"10"
	"$rimlightboost" ".25"

 	"$blendtintbybasealpha" "1" 
 	"$blendtintcoloroverbase" "0"  

	"$colortint_base" "{56 93 125}"
	"$colortint_tmp" "[0 0 0]"
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}