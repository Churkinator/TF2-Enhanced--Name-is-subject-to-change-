"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_targe_xmas_cables"
	"$bumpmap" "models/player/shared/shared_normal"

	"$phong" "1"
	"$phongexponent" "1"
	"$phongboost" ".1"
	"$phongfresnelranges" "[1 1 1]"	
	"$lightwarptexture" "models\lightwarps\xms_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" "7.5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}