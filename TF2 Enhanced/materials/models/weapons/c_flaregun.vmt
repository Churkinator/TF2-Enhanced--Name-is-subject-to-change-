"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_flaregun"
	"$basemapalphaphongmask" "1"

//	"$envmap" "env_cubemap"
//	"$envmaptint" "[.15 .15 .15]"
//	"$basemapenvmapmask" "1"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "10"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges" "[0.45 0.7 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "14"
	"$rimlightboost" ".86"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}