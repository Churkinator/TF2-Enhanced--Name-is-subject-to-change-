"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_rainblower"
	"$bumpmap" "models/weapons/c_items/c_rainblower_normal"

	">=DX90"
	{
		"$envmap" "cubemaps/cubemap_gold001"
		"$envmaptint" "[.718 .702 .436]"
		"$envmapsaturation" "1"	
	}

	"$phong" "1"
	"$phongexponent" "145"
	"$phongboost" "8.5"	
	"$lightwarptexture" "models\lightwarps\rainblower_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "20"
	"$rimlightboost" ".25"

	"$yellow" "0"
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}

