"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_sandwich_xmas"
	"$bumpmap" "models\weapons\c_items/c_sandwich_xmas_normal"

	"$selfillum" 1
	
	"$envmap" "env_cubemap"
//	"$envmaptint" "[.5 .5 .5]"

	"$lightwarptexture" "models/lightwarps/xms_lightwarp"

	//phong properties
	"$phong"	"1"
	"$phongboost"	"5"
	"$phongexponent"	"55"
	"$phongfresnelranges"	"[1 2 2.5]"
	"$halflambert" "1"

	"$rimlight"	"1"
	"$rimlightexponent"	"35"
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"Sine"
		{
			"resultVar"	"$selfillumtint" 
		    "sineperiod"	5
			"sinemin"       .3
			"sinemax"	1
		}
		"Sine"
		{
			"resultVar"	"$envmaptint" 
		    "sineperiod"	5
			"sinemin"       .2
			"sinemax"	.35
		}
	}
}