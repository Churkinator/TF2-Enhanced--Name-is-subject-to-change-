"VertexLitGeneric"
{
	"$basetexture" "models\bots\demo\demo_buster"
	"$bumpmap" "models\bots\demo\demo_buster_normal"

	"$selfillum" "1"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "2"	
	"$phongfresnelranges"	"[.5 .7 1]"
	"$lightwarptexture" "models\lightwarps\robot_lightwarp"
	"$halflambert" "1"
//	"$phongexponenttexture" "models\bots\classes\demo_bot\demo_bot_exp"
	
	"$rimlight" "1"						
	"$rimlightexponent" "4"				
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
	}
}
