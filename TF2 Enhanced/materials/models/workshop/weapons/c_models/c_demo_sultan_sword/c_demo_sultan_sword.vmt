"VertexLitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_demo_sultan_sword/c_demo_sultan_sword"
	"$bumpmap" "models/player/shared/shared_normal"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "2.5"
	"$phongfresnelranges" "[.25 .5 1]"
	"$halflambert" "1"
	"$lightwarptexture" "models\weapons\c_items/c_ambassador_lightwarp"
	
	// Rim lighting parameters
	"$rimlight" "1"					
	"$rimlightexponent" "7"		
	"$rimlightboost" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"WeaponSkin"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}