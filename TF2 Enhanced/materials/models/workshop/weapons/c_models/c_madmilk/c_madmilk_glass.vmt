"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_madmilk/c_madmilk_glass"
	"$bumpmap" "models/player/shared/shared_normal"

	"$additive" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"
	"$envmapsaturation" "1"
	"$normalmapalphaenvmapmask" "1"

	"$phong" "1"
	"$phongexponent" "100"
	"$phongboost" "6"	
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$phongfresnelranges" "[0.25 0.5 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "20"	
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"
	"$glowcolor" "1"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
