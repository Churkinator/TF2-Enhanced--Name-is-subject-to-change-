"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_scorch_shot/c_scorch_shot"
	"$basemapalphaphongmask" 1

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "3"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges" "[.75 1.5 3]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "14"	
	"$rimlightboost" ".86"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"WeaponSkin"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}