"VertexLitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_trenchgun/c_trenchgun.vtf"
	"$basemapalphaphongmask" "1"

    "$phong" "1"
    "$phongexponent" "17"
    "$phongboost" "9"
  	"$phongfresnelranges" "[.25 .5 1]"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"	
	"$halflambert" "1"

	"$commadd_phongexponent" "20"
	"$commadd_phongboost" 	"10"
	"$commweapon"		"0"

	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "10"	
	"$rimlightboost" ".55"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"


	"Proxies"
	{
		"invis"
		{
		}
		"WeaponSkin"
		{
		}				
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
