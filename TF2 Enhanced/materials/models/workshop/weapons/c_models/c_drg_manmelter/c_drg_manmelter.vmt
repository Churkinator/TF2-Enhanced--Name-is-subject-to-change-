"VertexlitGeneric"
{
	"$basetexture"	"models/workshop/weapons/c_models/c_drg_manmelter/c_drg_manmelter"
	"$bumpmap" "models/workshop/weapons/c_models/c_drg_manmelter/c_drg_manmelter_normal"
	
