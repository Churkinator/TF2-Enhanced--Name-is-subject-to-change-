"VertexlitGeneric"
{
	"$baseTexture" "models\workshop\weapons\c_models\c_kingmaker_sticky/c_kingmaker_sticky"
	"$basemapalphaphongmask" "1"

	"$phongtint" "[1 .9 .8]"
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".75"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[1.5 3 6]"


	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" ".6"

	"$yellow" "0"
	"$glowcolor" "1"

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}