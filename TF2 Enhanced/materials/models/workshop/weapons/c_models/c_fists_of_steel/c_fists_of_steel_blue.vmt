"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_fists_of_steel/c_fists_of_steel_blue"
	"$basemapalphaphongmask" "1"

	"$basealphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.2 0.5 0.9]"
//	"$envmaptint" "[0.713734491 0.748947941 0.761431507]"
	"$envmapsaturation" "1"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1"
	"$phongfresnelranges" "[.25 .55 3]"
	"$halflambert" "1"

	"$lightwarptexture" "models/workshop/weapons/c_models/c_fists_of_steel/c_fists_of_steel_lightwarp_blue"
	"$phongwarptexture"  "models/workshop/weapons/c_models/c_fists_of_steel/c_fists_of_steel_phongwarp_blue"

	"$rimlight" "1"
	"$rimlightexponent" "8"
	"$rimlightboost" ".2"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}