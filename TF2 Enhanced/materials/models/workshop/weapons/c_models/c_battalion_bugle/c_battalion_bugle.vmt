"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_battalion_bugle/c_battalion_bugle"
	"$bumpmap" "models/workshop/weapons/c_models/c_battalion_bugle/c_battalion_bugle_normal"
	
	"$phong" "1"
	"$phongexponent" "6"
	"$phongboost" "0.4"	
	"$phongfresnelranges" "[1.5 3 4.5]"
	"$halflambert" "1"
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"	

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}		
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}