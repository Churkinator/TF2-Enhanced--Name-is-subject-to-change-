"VertexLitGeneric"
{
	// intentionally points at the red skin so the player is holding an enemy version
	"$baseTexture" "models/workshop/weapons/c_models/c_skullbat/c_skullbat_hats"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1.5"
//	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges" "[.25 0.5 1]"
	"$halflambert" "1"

	// Rim lighting parameters
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}