"VertexLitGeneric"
{
	"$basetexture"		"models/workshop/player/items/all_class/robo_all_gibus/robo_all_gibus_color"
	"$bumpmap"		"models/workshop/player/items/all_class/robo_all_gibus/robo_all_gibus_normal"
	"$lightwarptexture"		"models/lightwarps/robot_lightwarp"
	"$phong"		"1"
	"$phongexponent"		"12"
	"$phongboost"		"4"
	"$phongfresnelranges"		"[.5 .7 1]"
	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"
	"$blendtintbybasealpha"		"1"
	"$blendtintcoloroverbase"		"1"
	"$colortint_base"		"{ 56 48 48 }"
	"$colortint_tmp"		"[0 0 0]"

	"$cloakPassEnabled"		"1"
	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0"
	"$detailblendmode"		"6"
	"$yellow"		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}