"VertexLitGeneric"
{
	"$basetexture"	"models/workshop/player/items/medic/jul13_positive_pressure_veil/jul13_positive_pressure_veil_color"
	"$bumpmap" "models/player/shared/shared_normal"

	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" "0.01"
	"$detailblendmode" "6"

	"$additive"	"1"

	">=DX90"
	{
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.235294 0.235294 0.235294]"
	"$normalmapalphaenvmapmask"	"1"
	"$envmapsaturation" "1"
	}

	"$phong" "1"
	"$phongexponent" "125"
	"$phongboost" "2"
	"$phongfresnelranges" "[.3 1 8]"
	"$halflambert" "1"

	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"

	"$blendtintbybasealpha"		"1"
	"$blendtintcoloroverbase"		"0"

	"$colortint_base"		"{155 85 0}"
	"$colortint_tmp"		"[0 0 0]"

	"$cloakPassEnabled"		"1"
	"$yellow"		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}