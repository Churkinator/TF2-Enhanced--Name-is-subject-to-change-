"VertexLitGeneric"
{
	"$basetexture"		"models/workshop/player/items/pyro/robo_pyro_electric_escorter/robo_pyro_electric_escorter_1_color"
	"$bumpmap"		"models/workshop/player/items/pyro/robo_pyro_electric_escorter/robo_pyro_electric_escorter_1_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" "0.01"
	"$detailblendmode" "6"

	"$lightwarptexture"	"models/lightwarps/robot_lightwarp"

	"$phong" "1"
	"$phongexponent" "12"
	"$phongboost" "7.75"
	"$phongfresnelranges" "[.5 .7 1]"
	"$halflambert" "1"

	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"

	"$blendtintbybasealpha"		"1"
	"$blendtintcoloroverbase"		"1"

	"$colortint_base"		"{215 168 72}"
	"$colortint_tmp"		"[0 0 0]"

	"$cloakPassEnabled"		"1"
	"$yellow"		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}