"VertexLitGeneric"
{
	"$basetexture" "models/workshop/player/items/pyro/robo_pyro_figment_filament/robo_pyro_figment_filament_1_color"
	"$bumpmap" "models/player/shared/shared_normal"

	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0.01"
	"$detailblendmode"		"6"

	"$additive"	"1"

	">=DX90"
	{
		"$envmap" "env_cubemap"
		"$normalmapalphaenvmapmask"	"1"
		"$envmaptint" "[0.2115 0.23 0.2115]"

		"$selfillum" "1"
		"$selfIllumFresnel"	"1"
		"$selfIllumFresnelMinMaxExp"		"[.1 2 3]"
		"$selfillumtint"		"[1 1 1]"
	}

	"$phong" "1"
	"$phongexponent" "200"
	"$phongboost" "25"
	"$phongfresnelranges" "[.3 1 8]"
	"$lightwarptexture"	"models/lightwarps/jarate_lightwarp"
	"$halflambert" "1"

	"$rimlight"	"1"
	"$rimlightexponent"	"4"
	"$rimlightboost" "2"

	"$blendtintbybasealpha"	"0"
	"$blendtintcoloroverbase" "0"

	"$colortint_base" "{100 87 76}"
	"$colortint_tmp" "[0 0 0]"

	"$cloakPassEnabled"	"1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"Divide"
		{
			"srcVar1"		"$colortint_base"
			"srcVar2"		"$colortint_tmp"
			"resultVar"		"$selfillumtint"
		}
		"Clamp"
		{
			"min"		"0.300000"
			"max"		"0.900000"
			"srcVar1"		"$selfillumtint"
			"resultVar"		"$selfillumtint"
		}
	}
}