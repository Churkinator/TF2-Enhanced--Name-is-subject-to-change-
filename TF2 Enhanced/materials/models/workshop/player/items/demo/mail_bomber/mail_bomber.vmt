"VertexLitGeneric"
{
	"$basetexture" "models/workshop/player/items/demo/mail_bomber/mail_bomber_color"
	"$basemapalphaphongmask" "1"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" "0.1"
	"$detailblendmode" "6"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "4"
	"$phongfresnelranges" "[.25 .5 1]"
	"$lightwarptexture"	"models/player/pyro/pyro_lightwarp"
	"$halflambert" "1"

	"$rimlight"	"1"
	"$rimlightexponent"	"4"
	"$rimlightboost" "2"

	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}