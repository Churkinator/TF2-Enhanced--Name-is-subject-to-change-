"VertexLitGeneric"
{
	"$basetexture"		"models/workshop/player/items/sniper/robo_sniper_solar_topi/robo_sniper_solar_topi_color"
	"$bumpmap"		"models/workshop/player/items/sniper/robo_sniper_solar_topi/robo_sniper_solar_topi_normal"

	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0.01"
	"$detailblendmode"		"6"

	"$lightwarptexture"		"models/lightwarps/robot_lightwarp"
	"$phong"		"1"
	"$phongexponent"		"12"
	"$phongboost"		"3.7"
	"$phongfresnelranges"		"[.3 1 8]"
	"$halflambert" "1"

	"$rimlight"	"1"
	"$rimlightexponent"	"4"
	"$rimlightboost" "1"

	"$blendtintbybasealpha"		"1"
	"$blendtintcoloroverbase"		"1.000000"

	"$colortint_base"		"{ 208 199 177 }"
	"$colortint_tmp"		"[0 0 0]"

	"$cloakPassEnabled"		"1"
	"$yellow"		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}