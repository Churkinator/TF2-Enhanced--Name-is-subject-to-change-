"VertexLitGeneric"
{
	"$basetexture"		"models/workshop/player/items/sniper/robo_sniper_liquidator/robo_sniper_liquidator_1_blue_color"
	"$basemapalphaphongmask" "1"

	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0.01"
	"$detailblendmode"		"6"

	"$phong"		"1"
