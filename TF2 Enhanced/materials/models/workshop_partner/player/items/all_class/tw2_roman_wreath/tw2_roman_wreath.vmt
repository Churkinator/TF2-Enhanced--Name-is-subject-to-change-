"VertexLitGeneric"
{
	"$basetexture" "models/workshop_partner/player/items/all_class/tw2_roman_wreath/tw2_roman_wreath_color"
	"$basemapalphaphongmask" "1"

	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0.01"
	"$detailblendmode"		"6"

	">=DX90"
	{
		"$envmap"		"cubemaps/cubemap_gold001"
		"$envmaptint"		"[.825 .7018 0]"
	}

	"$phong" "1"
	"$phongexponent" "80"
	"$phongboost" "10"
	"$phongfresnelranges"		"[.25 .5 1]"
	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"
	"$halflambert" "1"

	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"

	"$blendtintbybasealpha"		"1"
	"$blendtintcoloroverbase"		"0"

	"$colortint_base"		"{255 255 255}"
	"$colortint_tmp"		"[0 0 0]"


	"$cloakPassEnabled"		"1"
	"$yellow"		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}