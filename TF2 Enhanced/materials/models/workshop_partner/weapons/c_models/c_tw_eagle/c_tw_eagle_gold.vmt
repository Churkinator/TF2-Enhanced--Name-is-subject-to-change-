"VertexlitGeneric"
{
	"$basetexture" "models/workshop_partner/weapons/c_models/c_tw_eagle/c_tw_eagle"
	"$basemapalphaphongmask" "1"

	"$envmap" "effects/saxxy/saxxy_gold"
	"$envmaptint" "[.98 .883137 .36]"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongexponent" "100"
	"$phongboost" "7"
	"$phongtint" "{244 194 27}"
	"$phongfresnelranges" "[1.5 3 6]"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "6"
	"$rimlightboost" "1"

	// Cloaking
	"$glowcolor" "1"
	"$yellow" "0"
	"$cloakPassEnabled" "1"	
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}		
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
