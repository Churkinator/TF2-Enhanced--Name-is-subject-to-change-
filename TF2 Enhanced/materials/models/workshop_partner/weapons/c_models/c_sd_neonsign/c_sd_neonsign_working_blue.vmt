"VertexlitGeneric"
{
	"$baseTexture" "models/workshop_partner/weapons/c_models/c_sd_neonsign/c_sd_neonsign"
	"$bumpmap" "models/workshop_partner/weapons/c_models/c_sd_neonsign/c_sd_neonsign_normal"
	">=DX90"
	{
		"$selfillum" "1"
	}

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"
	
	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"invis"
		{
		}
		"Sine"
		{
			"resultVar"	"$selfillumtint" 
		    "sineperiod" 1.5
			"sinemin" 0
			"sinemax" 33
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}