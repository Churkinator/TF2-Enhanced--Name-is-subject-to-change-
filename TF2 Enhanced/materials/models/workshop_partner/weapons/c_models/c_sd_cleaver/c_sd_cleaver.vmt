"VertexLitGeneric"
{
	"$baseTexture" "models/workshop_partner/weapons/c_models/c_sd_cleaver/c_sd_cleaver"
	"$basemapalphaphongmask" "1"

	"$basemapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.35 .35 .35]"
	"$envmapsaturation" "1"

	"$phong" "1"
	"$phongboost" "3"
	"$phongexponent" "25"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges" "[.5 1 3]"
	"$halflambert" "1"

	"$tempvar" "0"

	"$basephongexponent" "45"
	"$basephongboost" "10"

	"$commweapon"	"0"
	"$commadd_phongexponent" "30"
	"$commadd_phongboost" "22.5"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}