"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/crafting/stat_clock_red_sad"
	"$bumpmap" "models/player/shared/shared_normal"

	"$selfillum" "1"

	"$detail" "models/weapons/c_items/stattracker_lines"
	"$detailscale" "1"
	"$detailblendmode" "8"
	"$detailblendfactor" "1.2"

	"$envmap" "env_cubemap"
	"$envmaptint" "[0.4 0.4 0.4]"
	"$normalmapalphaenvmapmask" "1"

	"$lightwarptexture" "models\buildables\sentry1\Sentry1_lightwarp"
	
	"$glowcolor" "1"
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"StatTrakIcon"
		{
			"resultVar" "$basetexturetransform"
		}
		"TextureScroll"
		{
		"texturescrollvar" "$detailtexturetransform"
		"texturescrollrate" -0.075
		"texturescrollangle" 90
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}	
	}
}
