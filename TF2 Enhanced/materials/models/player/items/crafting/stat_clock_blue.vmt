"VertexLitGeneric"
{
	"$baseTexture" "models/player/items/crafting/stat_clock_blue"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongboost" ".15"
	"$phongexponent" "5"
	"$lightwarptexture" "models\buildables\sentry1\Sentry1_lightwarp"
	"$phongfresnelranges"	"[.75 1.5 3]"
	"$halflambert" "1"

	"$rimlight" "1"		
	"$rimlightexponent" "10"				
	"$rimlightboost" "1.25"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"
	"$glowcolor" "1"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}