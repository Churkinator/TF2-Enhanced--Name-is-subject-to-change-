"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/all_class/fob_heavy"
	"$bumpmap" "models/player/shared/shared_normal"

	">=DX90"
	{
		"$selfillum" "1"
	}

	"$phong" "1"
	"$phongexponent" "55"
	"$phongboost" ".22"
	"$phongfresnelranges" "[.5 .7 1]"
	"$halflambert" "1"

	"$colortint_base" "{28 237 228}"
	"$colortint_tmp" "[0 0 0]"
	
	"$rimlight" "1"
	"$rimlightexponent" "25"
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}