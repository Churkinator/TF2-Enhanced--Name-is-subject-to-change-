"VertexLitGeneric"
{
	"$basetexture" "models/player/items/mvm_loot/all_class/fob_heavy_gold"
	"$basemapalphaphongmask" "1"

	">=DX90"
	{
		"$envmap" "cubemaps/cubemap_gold001"
		"$envmaptint" "[2.325 1.725 1.625]"
		"$envmapsaturation" "1"		
	}

	"$phong" "1"
	"$phongexponent" "160"
	"$phongboost" "4.7"
	"$phongfresnelranges" "[.5 .7 1]"
	"$halflambert" "1"	
	"$lightwarptexture" "models/lightwarps/robot_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "25"
	"$rimlightboost" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$glowcolor" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}