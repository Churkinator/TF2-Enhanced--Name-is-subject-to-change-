"VertexlitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/all_class/fob_heavy_diamond"
	"$basemapalphaphongmask" "1"

	">=DX90"
	{
	"$envmap" "env_cubemap"
	"$envmaptint" "[.35 .35 .35]"
	}

	"$additive" "1"
//	"$translucent" 1

	"$phong" "1"
	"$phongexponent" "100"
	"$phongboost" "10"
	"$lightwarptexture" "models/player/items/all_class/all_class_ring_warp"	
	"$phongfresnelranges"	"[1 1 1]"
	"$phongwarptexture" "models/player/items/all_class/all_class_ring_warp"
	"$halflambert" "1"

	// Rim lighting parameters
	"$rimlight" "1"
	"$rimlightexponent" "150"
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}