"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/all_class/fob_heavy"
	"$bumpmap" "models/player/items/mvm_loot/all_class/fob_heavy_normal"

	"$phong" "1"
	"$phongexponent" "55"
	"$phongboost" "4.7"
	"$phongfresnelranges" "[.5 .7 1]"
	"$halflambert" "1"	
	"$lightwarptexture" "models/lightwarps/robot_lightwarp"
	
	"$rimlight" "1"
	"$rimlightexponent" "25"
	"$rimlightboost" "1"
	
 	"$blendtintbybasealpha" "1" 
	"$blendtintcoloroverbase" "0"  

	"$colortint_base" "{242 166 66}"
	"$colortint_tmp" "[0 0 0]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}