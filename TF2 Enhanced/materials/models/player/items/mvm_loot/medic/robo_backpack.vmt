"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/medic/robo_backpack"
	"$basemapalphaphongmask" "1"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$phong" "1"
	"$phongexponent" "12"
	"$phongboost" "5"
	"$lightwarptexture" "models/lightwarps/robot_lightwarp"
	"$phongfresnelranges" "[.3 1 8]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
	}
}