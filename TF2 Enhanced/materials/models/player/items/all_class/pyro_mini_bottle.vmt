"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/all_class/pyro_mini_bottle"
	"$bumpmap" "models/player/items/all_class/pyro_mini_bottle_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" 0.1
	"$detailblendmode" 6

	"$envmap" "effects/pyrocube.hdr"
	"$envmaptint" "[.2 .2 .2]"
	"$envmapsaturation" "1"
//	"$basealphaenvmapmask" "1"

	//phong properties
	"$phong" "1"
	"$phongboost" "1"
	"$phongexponent" "20"
	"$phongfresnelranges"	"[1 2 4]"
	"$lightwarptexture" "models/lightwarps/rainblower_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "2"

	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}