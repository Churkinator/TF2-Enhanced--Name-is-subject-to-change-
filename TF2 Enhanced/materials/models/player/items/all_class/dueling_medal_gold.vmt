"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/dueling_medal_gold"
	"$bumpmap" "models/player/shared/shared_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .1
	"$detailblendmode" 6

	"$envmap" "env_cubemap"
	"$envmaptint" "[.90756 .8 .38]"
	"$envmapsaturation" "1"
	
	"$phong" "1"
	"$phongexponent" "80"
	"$phongboost" "4.5"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"	
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"

	// Rim lighting parameters
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "2"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"
	
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}